<?xml version="1.0" encoding="utf-8"?>
<!-- Adam Sandilands s2032103-->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DataActivity">


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:background="#FFFFFF"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="5dp">

        <ImageView
            android:id="@+id/traffic_act_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="horizontal">

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="horizontal">

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="horizontal">

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="horizontal">

        </LinearLayout>

    </LinearLayout>

    <TextView
        android:id="@+id/traffic_road"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="141dp"
        android:layout_marginTop="16dp"
        android:text="Road: "
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/traffic_title_display"
        style="style/word_content"
        android:layout_width="302dp"
        android:layout_height="21dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="15dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/traffic_road" />

    <TextView
        android:id="@+id/traffic_description2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="172dp"
        android:text="Description"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/traffic_description_display"
        style="style/word_content"
        android:layout_width="321dp"
        android:layout_height="82dp"
        android:layout_marginStart="118dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="60dp"
        app:layout_constraintBottom_toTopOf="@+id/publised"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/traffic_title_display"
        app:layout_constraintTop_toBottomOf="@+id/traffic_description2" />

    <TextView
        android:id="@+id/startDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="43dp"
        android:layout_marginBottom="24dp"
        android:layout_weight="1"
        android:text="Start date"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/startDate_display"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/traffic_title_display" />

    <TextView
        android:id="@+id/startDate_display"
        style="style/word_content"
        android:layout_width="78dp"
        android:layout_height="0dp"
        android:layout_marginStart="30dp"
        android:layout_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="158dp" />

    <TextView
        android:id="@+id/endDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="81dp"
        android:layout_marginTop="44dp"
        android:layout_weight="1"
        android:text="End Date"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/startDate"
        app:layout_constraintTop_toBottomOf="@+id/traffic_title_display" />

    <TextView
        android:id="@+id/endDate_display"
        style="style/word_content"
        android:layout_width="99dp"
        android:layout_height="30dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="558dp"
        android:layout_marginBottom="70dp"
        android:layout_weight="1"
        app:layout_constraintBottom_toTopOf="@+id/coordinates"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/startDate_display"
        app:layout_constraintTop_toBottomOf="@+id/endDate" />

    <TextView
        android:id="@+id/link"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="73dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="348dp"
        android:layout_marginBottom="25dp"
        android:text="Link"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/link_display"
        app:layout_constraintEnd_toStartOf="@+id/publised"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/startDate_display" />

    <TextView
        android:id="@+id/link_display"
        style="style/word_content"
        android:layout_width="118dp"
        android:layout_height="41dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="132dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/coordinates"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="117dp"
        android:layout_marginBottom="16dp"
        android:text="cooardates"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/coordinates_displau"
        app:layout_constraintEnd_toStartOf="@+id/published_display" />

    <TextView
        android:id="@+id/coordinates_displau"
        style="style/word_content"
        android:layout_width="175dp"
        android:layout_height="83dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="72dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/day_No"
        app:layout_constraintStart_toEndOf="@+id/link_display" />

    <TextView
        android:id="@+id/publised"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="274dp"
        android:text="Pushlished on"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/traffic_description_display" />

    <TextView
        android:id="@+id/published_display"
        style="style/word_content"
        android:layout_width="196dp"
        android:layout_height="24dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="205dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/publised" />

    <TextView
        android:id="@+id/day_No"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="135dp"
        android:text="No of Days"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/dayNo_display"
        app:layout_constraintTop_toBottomOf="@+id/published_display" />

    <TextView
        android:id="@+id/dayNo_display"
        style="style/word_content"
        android:layout_width="200dp"
        android:layout_height="29dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="135dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/published_display" />

</androidx.constraintlayout.widget.ConstraintLayout>